<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title></title>
    <style type="text/css">
        html {
            margin: 0;
            padding: 0;
        }
        body {
            margin: 0;
            padding: 0;
            max-width: 100%;
        }

        @font-face {
            font-family: 'OCR-B 10 BT';
            font-style: normal;
            font-weight: normal;
            src: url(data:font/truetype;charset=utf-8;base64,) format("opentype");
        }

        #esr_outer {
            max-width: 100%;
            position: relative;
        }

        #esr_inner {
            position: relative;
        }

        #esr_outer {
            font-family: Arial, Helvetica, sans-serif;
            font-weight: bold;
            font-size: 12pt;
        }
        pre {
            font-family: 'OCR-B 10 BT';
            margin: 0;
            font-size: 14pt;
            font-weight: normal;
        }

        /**
         * Adressen
         */
        #esr_bank_left,
        #esr_bank_right,
        #esr_receiver_left,
        #esr_receiver_right,
        #esr_sender_left,
        #esr_reference_number_left,
        #esr_sender_right {
            position: absolute;
            width: 6.5cm;
        }

        #esr_receiver_left,
        #esr_receiver_right,
        #esr_bank_left,
        #esr_bank_right {
            top: 1.1cm;
        }

        {% if data.isBesr %}
        #esr_receiver_left,
        #esr_receiver_right {
            top: 3.0cm;
        }
        {% endif %}

        #esr_receiver_left,
        #esr_bank_left,
        #esr_reference_number_left,
        #esr_sender_left {
            left: 0.5cm;
        }

        #esr_receiver_right,
        #esr_bank_right {
            left: 8.3cm;
        }

        #esr_reference_number_left {
            font-family: 'OCR-B 10 BT';
            margin: 0;
            font-size: 12pt;
            font-weight: normal;
            top: 7.9cm;
        }

        #esr_sender_left {
            top: 9.2cm;
        }

        #esr_sender_right {
            left: 17.4cm;
            top: 6.7cm;
        }

        /**
         * Code-Zeilen
         */
        #esr_reference_number {
            position: absolute;
            left: 16.2cm;
            top: 4.35cm;
            height: 0.7cm;
            width: 10.2cm;
            text-align: right;
            line-height: 0.7cm;
            vertical-align: middle;

        }

        /**
         * Kontonummer
         */
        #esr_account_left,
        #esr_account_right {
            position: absolute;
            top: 5.5cm;
        }
        #esr_account_left {
            left: 3.6cm;
        }
        #esr_account_right {
            left: 11.5cm;
        }

        /**
         * Preis
         */
        #esr_price_left,
        #esr_price_right,
        #esr_price_decimal_left,
        #esr_price_decimal_right {
            position: absolute;
            top: 6.6cm;
            height: 0.55cm;
            line-height: 0.7cm;
            vertical-align: middle;
            /*background: #00a65a;*/
        }
        #esr_price_left pre,
        #esr_price_right pre,
        #esr_price_decimal_left pre,
        #esr_price_decimal_right pre {
            font-size: 15pt;
        {% if data.isBoxed %}
            letter-spacing: 0.35cm;
            margin-right: -0.1cm;
        {% endif %}
        }

        #esr_price_left,
        #esr_price_right {
            width: 4cm;
            text-align: right;
        }

        #esr_price_decimal_left,
        #esr_price_decimal_right {
            width: 0.9cm;
            padding-left: 0.1cm;
        }

        #esr_price_left {
            left: 0.38cm;
        }

        #esr_price_decimal_left {
            left: 6.2cm;
        }

        #esr_price_right {
            left: 8.28cm;
        }

        #esr_price_decimal_right {
            left: 14.15cm;
        }

        {% set base = 0.6 %}
        {% for i in range(data.code|length, 0) %}
        #esr_code_{{i}}_letter {
            right: {{ base }}cm;
        {% set base = base + 0.29 %}
        }
        {% endfor %}


        #esr_code pre {
            position: absolute;
            top: 11.3cm;
        }

        {% if data.showEsrBackground %}
        /**
         * Hintergrund
         */
        #background {
            max-width: 100%;
        }
        {% else %}
        /**
         * Korrekturen für Druck
         */
        #esr_inner {
            top: 0.1cm;
        }
        {% endif %}

    </style>

</head>
<body>
{% include '@whatwedoEsr/_esr.html.twig' with { 'data': data } only %}
</body>
</html>

